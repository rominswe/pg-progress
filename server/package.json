{
  "name": "aiu-pg-progress-server",
  "version": "1.0.0",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=production node src/server.js",
    "dev": "NODE_ENV=development nodemon src/server.js",
    "staging": "NODE_ENV=staging node src/server.js",
    "dev:staging": "NODE_ENV=staging nodemon src/server.js",
    "seed": "NODE_ENV=development node src/seeds/seedadmin.js",
    "seed:staging": "NODE_ENV=staging node src/seeds/seedadmin.js",
    "migrate": "NODE_ENV=development node src/migrations/run_migrations.js",
    "migrate:staging": "NODE_ENV=staging node src/migrations/run_migrations.js",
    "migrate:prod": "NODE_ENV=production node src/migrations/run_migrations.js",
    "security-audit": "npm audit --audit-level=moderate",
    "security-test": "node -e \"console.log('Security tests would run here')\""
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^2.2.1",
    "connect-redis": "^9.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crud": "^0.0.28",
    "dotenv": "^16.3.1",
    "express": "^4.22.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "express-session": "^1.18.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "mysql2": "^3.15.3",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.11",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "sequelize-auto": "^0.8.8"
  }
}
